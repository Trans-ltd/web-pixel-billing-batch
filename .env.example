# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id

# Workload Identity Federation Configuration
# These values are generated by running ./scripts/setup-wif.sh
WIF_PROVIDER=//iam.googleapis.com/projects/YOUR_PROJECT_NUMBER/locations/global/workloadIdentityPools/github-pool/providers/github-provider
WIF_SERVICE_ACCOUNT=github-actions-deploy@your-project-id.iam.gserviceaccount.com

# Application Configuration
NODE_ENV=production

# Shopify API Configuration
SHOPIFY_API_VERSION=2024-01

# Batch Processing Configuration
BATCH_SIZE=5
MAX_RETRIES=3
API_TIMEOUT_SECONDS=30


# Cloud Functions Configuration
FUNCTION_MEMORY=512Mi
FUNCTION_TIMEOUT=540s
FUNCTION_CPU=1